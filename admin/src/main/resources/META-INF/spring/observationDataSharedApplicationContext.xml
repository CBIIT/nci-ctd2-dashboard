  <!-- columbiaTier4GliomaStoryReader defined in: -->
  <!-- observationDataApplicationContext.xml testObservationDataApplicationContext.xml -->
  <bean name="columbiaTier4GliomaStoryLineMapper" class="org.springframework.batch.item.file.mapping.DefaultLineMapper">
